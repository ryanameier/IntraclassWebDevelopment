	<section id="contact" class="module">
		<div class="container">
			<div class="row">

				<div class="col-sm-6 col-sm-offset-3">

					<div class="module-header">
						<h2 class="module-title">Contact</h2>
						<div class="module-line"></div>
						<div class="module-subtitle">
							Ready to get started? Put in your contact information to send an immediate email straight to our gmail. Please include a good time to contact you, if its a weekday between 9-5pm PST we can usually reach out same day! We are all about transparency, so whether you're a current or new client, we're here to help!
						</div>
						<div class="module-line"></div>
					</div>

				</div>

			</div><!-- .row -->

			<div class="row">

				<div class="col-sm-6 col-sm-offset-3">

				<%= form_for @contact, :url => contacts_path, :html => {:class => 'contact-form'} do |f| %>

				<!--	<form id="contact-form" role="form"> -->
						<div class="ajax-hidden">
							<div class="form-group wow fadeInUp">
								<%= f.label :name, class: "sr-only" %>
								<!-- <label class="sr-only" for="c_name">Name</label> !-->
								<%= f.text_field :name, autocomplete: "off", class: "form-control", placeholder: "Name" %>
								<!-- <input type="text" id="c_name" class="form-control" name="c_name" placeholder="Name"> !-->
							</div>

							<div class="form-group wow fadeInUp" data-wow-delay=".1s">
							<%= f.label :email, class: "sr-only" %>
								<!-- <label class="sr-only" for="c_email">Email address</label> !-->
								<%= f.text_field :email, autocomplete: "off", class: "form-control", placeholder: "Email" %>
								<!-- <input type="email" id="c_email" class="form-control" name="c_email" placeholder="E-mail"> !-->
							</div>

							<div class="form-group wow fadeInUp" data-wow-delay=".2s">
							<%= f.text_area :message, autocomplete: "off", class: "form-control txtfield", placeholder: "Your message" %>
								<!-- <textarea class="form-control" id="c_message" name="c_message" rows="7" placeholder="Your message"></textarea> !-->
							</div>
              <%= f.submit :submit, :class => 'btn btn-block btn-custom-2 wow fadeInUp'%>
							<!-- <button type="submit" class="btn btn-block btn-custom-2 wow fadeInUp" data-wow-delay=".3s">Send it
							</button>!-->
						</div>
						<div class="ajax-response"></div>
						<% end %>
				<!--	</form> !-->



				</div>

			</div><!-- .row -->
		</div><!-- .container -->
	</section>

